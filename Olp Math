//求回文数（左右对称）
#include<stdio.h>

int fun(long n)
{
	long i, k;
	i = n, k = 0;
	while (i)
	{
		k = i % 10 + k * 10;
		i = i / 10;
	}
	if (k == n)
		return 1;
	else
		return 0;
}
int main()
{
	long int m;
		for (m = 1; m < 1000; m++)
			if (fun(m) && fun(m * m) && fun(m * m * m))
				printf("m=%d,\tm*m=%d,\tm*m*m=%d\n",m,m*m,m*m*m);	
		return 0;
}
